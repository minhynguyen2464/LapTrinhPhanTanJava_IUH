package cau2;
import java.net.*;
import java.io.*;

public class client {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		final int SERVER_PORT = 6666;
		final String SERVER_NAME = "localhost";
		Socket clientSocket = null;
		try {
			clientSocket = new Socket(SERVER_NAME,SERVER_PORT);
			//Send
			
			clientSocket.close();
		}
		catch(Exception e) {
			e.printStackTrace();
		}
	}

}
